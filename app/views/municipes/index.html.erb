<h1>Listagem de Munícipes</h1>

<table>
  <thead>
    <tr>
      <th>Nome Completo</th>
      <th>CPF</th>
      <th>CNS</th>
      <th>Email</th>
      <th>Data de Nascimento</th>
      <th>Endereço</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% @municipes.each do |municipe| %>
      <tr>
        <td><%= municipe.nome_completo %></td>
        <td><%= municipe.cpf %></td>
        <td><%= municipe.cns %></td>
        <td><%= municipe.email %></td>
        <td><%= municipe.data_nascimento %></td>
        <td><%= "#{municipe.endereco.rua}, #{municipe.endereco.numero}, #{municipe.endereco.cidade}, #{municipe.endereco.estado}" %></td>
        <td><%= municipe.status ? 'Ativo' : 'Inativo' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
